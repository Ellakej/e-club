{
  "name": "e-club",
  "description": "Interactive game with A-frame and Tensorflow.js",
  "version": "0.2",
  "license": "MIT",
  "scripts": {
    "watch": "cross-env NODE_ENV=development parcel index.html --no-hmr ",
    "start": "live-server",
    "build": "cross-env NODE_ENV=production parcel build index.html --public-url ./",
    "lint": "eslint .",
    "link-local": "yalc link @tensorflow-models/handpose"
  },
  "devDependencies": {
    "babel-core": "~6.26.3",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.7.0",
    "clang-format": "~1.2.2",
    "cross-env": "^5.2.0",
    "dat.gui": "~0.7.2",
    "eslint": "~4.19.1",
    "eslint-config-google": "~0.9.1",
    "parcel-bundler": "~1.12.4",
    "yalc": "~1.0.0-pre.23"
  },
  "keywords": [
    "aframe",
    "aframe-example",
    "aframe-boilerplate",
    "aframe-scene",
    "webvr",
    "vr",
    "tensorflow",
    "hand-recognition",
    "interactive-game"
  ],
  "dependencies": {
    "@tensorflow-models/handpose": "0.0.5",
    "@tensorflow/tfjs": "2.0.1",
    "@tensorflow/tfjs-backend-wasm": "2.0.1",
    "live-server": "^1.2.1"
  }
}
